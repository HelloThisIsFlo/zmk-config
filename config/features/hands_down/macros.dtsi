ZMK_MACRO(Qu,
        wait-ms = <10>;
        tap-ms = <10>;
        bindings = <&macro_tap &kp Q>
                , <&macro_release &kp LSHFT>
                , <&macro_release &kp RSHFT>
                , <&macro_tap &kp U>
                ;
)
ZMK_MACRO(logseqPageRef,
        wait-ms = <100>;
        tap-ms = <10>;
        bindings = 
        <&kp LEFT_BRACKET>,
        <&kp LEFT_BRACKET>;
)
ZMK_MACRO(arrowLeftSingle,
        wait-ms = <10>;
        tap-ms = <10>;
        bindings = 
        <&kp LESS_THAN>,
        <&kp MINUS>;
)
ZMK_MACRO(arrowRightSingle,
        wait-ms = <10>;
        tap-ms = <10>;
        bindings = 
        <&kp MINUS>,
        <&kp GREATER_THAN>;
)
ZMK_MACRO(arrowLeftDouble,
        wait-ms = <10>;
        tap-ms = <10>;
        bindings = 
        <&kp LESS_THAN>,
        <&kp EQUAL>;
)
ZMK_MACRO(arrowRightDouble,
        wait-ms = <10>;
        tap-ms = <10>;
        bindings = 
        <&kp EQUAL>,
        <&kp GREATER_THAN>;
)